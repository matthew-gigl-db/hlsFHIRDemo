    "displayName" : "Claim Details",
    "query" : "with claim_fixed as (\n  select distinct\n    bundle_uuid\n    ,id::string as claim_id\n    ,use::string as use\n    ,array(\n      diagnosis:[0].diagnosisCodeableConcept.coding[0].code::string\n      ,diagnosis:[1].diagnosisCodeableConcept.coding[0].code::string\n      ,diagnosis:[2].diagnosisCodeableConcept.coding[0].code::string\n    ) as diag_codes\n    ,diagnosis:[0].diagnosisCodeableConcept.coding[0].code::string as primary_diag_code\n    ,priority:coding[0].code::string as priority\n    ,type:coding[0].code::string as claim_type\n  from \n    claim\n  where\n    bundle_uuid = :bundle_uuid\n)\nselect \n  *\n  ,ai_query(\n    endpoint => 'databricks-meta-llama-3-3-70b-instruct'\n    ,request => \"Lookup the ICD10 CM code descriptions for ('\" \n      || array_join(diag_codes, \"', '\" ) \n      || \"'). Then summarize the claims diagnosis findings in less than 250 characters.'\"\n    ,returnType => 'STRING'\n  ) AS diagnosis_code_summary\nfrom claim_fixed",
