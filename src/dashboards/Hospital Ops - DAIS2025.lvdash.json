    "query" : "select \n    case when id::string is not null then id::string else encounter_url end as encounter_id\n    ,type:[0].text::string as primary_encounter_type\n    ,class:code::string as encounter_class\n    -- ,split(appointment:[0].reference::string, \"/\")[1] as appointment_id\n    ,reasonCode:[0].coding[0].display::string as primary_reason\n    ,location:[0].location.display::string as primary_location\n    ,period:start::timestamp as encounter_start\n    ,period:end::timestamp as encounter_end\n    ,status::string as encounter_status\n    ,datediff(MINUTE, period:start::timestamp, period:end::timestamp) as duration_minutes\nfrom encounter",
